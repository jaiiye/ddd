<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/view/jsp/_layouts">
	<decorator name="none">
		<!-- These files will not get decorated. -->
		<pattern>/static/</pattern>
	</decorator>
	<decorator name="container" page="container.jsp">
		<pattern>/snippet/*</pattern>
	</decorator>
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="dialog" page="_snippets/dialog.jsp"/>
	<decorator name="grid" page="_snippets/grid.jsp"/>
	<decorator name="panel" page="_snippets/panel.jsp"/>
</decorators>